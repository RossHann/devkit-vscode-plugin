                    "command": "extension.view.portdeletereport",
                    "when": "view == portingTools && viewItem == portreport",
                    "group": "inline@3"
                },
                {
                    "command": "extension.view.portclearreports",
                    "when": "view == portingTools && viewItem == portsource && hasportreports",
                    "group": "inline@2"
                },
                {
                    "command": "extension.view.portCreateSoftWareBuildTask",
                    "when": "view == portingTools && viewItem == softBuild",
